{
    "name": "firmantr3/laravel-midtrans",
    "description": "Midtrans Payment API Wrapper for Laravel.",
    "type": "library",
    "require": {
        "midtrans/midtrans-php": "2.5",
        "illuminate/container": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/contracts": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13|^7.0",
        "phpunit/phpunit": "^9.5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Firman Taruna Nugraha",
            "email": "firmantr3@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Firmantr3\\Midtrans\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Firmantr3\\Midtrans\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firmantr3\\Midtrans\\Providers\\MidtransServiceProvider"
            ]
        }
    }
}
